{
  "manifest_version": 2,
  "name": "文章收藏",
  "description": "收藏文章，可以将打开的文章稍后读",
  "version": "1.0",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab",
    "webRequest", // web请求
    "webRequestBlocking", // 阻塞式web请求
    "storage", // 插件本地存储
    "http://*/*", 
    "<all_urls>",
    "cookies"
  ]
}